{
	"env": {
		"browser": true,
		"es6": true,
		"node": true
	},
	"extends": ["airbnb/base"],
	"rules": {
		"semi": "off",
		"no-console": "error",
		"no-plusplus": "off",
		"eol-last": "off",
		"max-len": ["error", 150, { "ignoreUrls": true}],
		"no-tabs": "off",
		"no-alert": "error",
		"radix": "off",
		"prefer-destructuring": "off",
		"indent": ["off", "tab"],
		"no-param-reassign": "off",
		"quotes": "off",
		"linebreak-style": ["error", "windows"],
		"import/prefer-default-export": "off",
		"consistent-return": "off",
		"default-case": "off"
	}
}